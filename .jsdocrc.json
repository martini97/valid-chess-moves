{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": "src/",
    "exclude": "src/client",
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "src/client"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true
  }
}
